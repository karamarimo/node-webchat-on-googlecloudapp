    "init-cloudsql": "node ./src/server/sql-model.js",
    "build": "webpack -p",
    "build:dev": "webpack -d",
    "build:watch": "webpack -d --watch"
  },
  "dependencies": {
    "bcrypt": "^1.0.2",
    "body-parser": "~1.17.1",
    "express": "^4.15.4",
    "jsonwebtoken": "^7.4.3",
    "lodash": "~4.17.4",
    "mysql": "^2.14.1",
    "nconf": "~0.8.4",
    "prompt": "~1.0.0",
    "socket.io": "^2.0.3"
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "axios": "^0.16.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "bootstrap": "^3.3.7",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "jquery": "^3.2.1",
    "proxyquire": "^1.8.0",
    "sinon": "^2.4.1",
    "socket.io-client": "^2.0.3",
    "vue": "^2.4.2",
    "vue-loader": "^13.0.4",
    "vue-router": "^2.7.0",
    "vue-template-compiler": "^2.4.2",
